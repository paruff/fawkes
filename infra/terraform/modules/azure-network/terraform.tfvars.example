# Example configuration for Azure Network module

vnet_name           = "fawkes-vnet"
location            = "eastus2"
resource_group_name = "fawkes-rg"
address_space       = ["10.0.0.0/16"]

subnet_name             = "aks-subnet"
subnet_address_prefixes = ["10.0.1.0/24"]

tags = {
  platform    = "fawkes"
  managed_by  = "terraform"
  environment = "dev"
}
