# Example Terraform variables for GCP Logging
# Copy this file to terraform.tfvars and update with your values

# GKE Cluster Configuration
cluster_name = "fawkes-prod"
project_id   = "your-gcp-project-id"
region       = "us-central1"

# OpenSearch Integration (optional)
# opensearch_endpoint = "https://opensearch.fawkes.svc.cluster.local:9200/_bulk"

# Resource Tags
tags = {
  Environment = "production"
  Team        = "platform"
  ManagedBy   = "terraform"
  Project     = "fawkes"
}
