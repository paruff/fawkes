          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "green", "value": 75 }
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 44,
        "title": "Avg Experiment ROI",
        "type": "stat",
        "gridPos": { "x": 18, "y": 33, "w": 6, "h": 6 },
        "description": "Average ROI percentage of completed experiments",
        "targets": [
          {
            "expr": "discovery_experiments_avg_roi_percentage",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "$datasource" }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "green", "value": 100 }
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
