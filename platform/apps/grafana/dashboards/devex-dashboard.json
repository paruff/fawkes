{
  "dashboard": {
    "title": "Developer Experience (DevEx) Dashboard",
    "tags": ["devex", "space", "developer-experience", "metrics"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5m",
    "description": "Comprehensive Developer Experience dashboard showing all 5 SPACE dimensions: Satisfaction, Performance, Activity, Communication, and Efficiency & Flow",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "id": 1,
        "title": "DevEx Overview",
        "type": "row",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Overall DevEx Health Score",
        "type": "stat",
        "gridPos": {"x": 0, "y": 1, "w": 6, "h": 4},
        "description": "Overall developer experience health score based on all SPACE dimensions (0-100 scale)",
        "targets": [
          {
            "expr": "space_devex_health_score{team=~\"$team\"}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "orange", "value": 50},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            },
            "mappings": []
          },
          "overrides": []
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 3,
        "title": "SPACE Dimensions Status",
        "type": "bargauge",
        "gridPos": {"x": 6, "y": 1, "w": 18, "h": 4},
        "description": "Health status of all 5 SPACE dimensions (0-5 scale)",
        "targets": [
          {
            "expr": "space_satisfaction_score{team=~\"$team\"}",
            "refId": "A",
            "legendFormat": "Satisfaction",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "space_performance_score{team=~\"$team\"}",
            "refId": "B",
            "legendFormat": "Performance",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "space_activity_score{team=~\"$team\"}",
            "refId": "C",
            "legendFormat": "Activity",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "space_communication_score{team=~\"$team\"}",
            "refId": "D",
            "legendFormat": "Communication",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "space_efficiency_score{team=~\"$team\"}",
            "refId": "E",
            "legendFormat": "Efficiency & Flow",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 5,
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "orange", "value": 2.5},
                {"color": "yellow", "value": 3.5},
                {"color": "green", "value": 4.0}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 10,
