{
  "dashboard": {
    "title": "Experimentation Framework - A/B Testing Analytics",
    "tags": ["experimentation", "ab-testing", "analytics"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Active Experiments",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "experimentation_experiments_active",
            "legendFormat": "Active"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Experiments by Status",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 18, "h": 4},
        "targets": [
          {
            "expr": "sum by (status) (experimentation_experiments_total)",
            "legendFormat": "{{status}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "colorMode": "value"
        }
      },
      {
        "id": 3,
        "title": "Variant Assignment Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(experimentation_variant_assignments_total[5m])",
            "legendFormat": "{{experiment_id}} - {{variant}}"
          }
        ],
        "yaxes": [
          {"label": "Assignments/sec", "format": "short"},
          {"label": "", "format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 4,
        "title": "Event Tracking Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(experimentation_events_total[5m])",
            "legendFormat": "{{experiment_id}} - {{event_name}}"
          }
        ],
        "yaxes": [
          {"label": "Events/sec", "format": "short"},
          {"label": "", "format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 5,
        "title": "Variant Assignment Distribution",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (variant) (experimentation_variant_assignments_total)",
            "legendFormat": "{{variant}}"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "donut"
        }
      },
      {
        "id": 6,
        "title": "Statistical Significance Results",
        "type": "stat",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "experimentation_significant_results_total",
            "legendFormat": "{{experiment_id}}"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "grey"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Analysis Duration",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(experimentation_analysis_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{experiment_id}}"
          },
          {
            "expr": "histogram_quantile(0.50, rate(experimentation_analysis_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{experiment_id}}"
          }
        ],
        "yaxes": [
          {"label": "Duration (seconds)", "format": "s"},
          {"label": "", "format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 8,
        "title": "Event Values Distribution",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 28, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(experimentation_event_values_bucket[5m])) by (le, experiment_id)",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {"xBuckets": {"mode": "size", "value": "1m"}}
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "experiment_id",
          "type": "query",
          "label": "Experiment",
          "query": "label_values(experimentation_variant_assignments_total, experiment_id)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m"]
    }
  }
}
