{
  "dashboard": {
    "title": "NASA-TLX Cognitive Load Assessment",
    "tags": ["nasa-tlx", "cognitive-load", "devex", "developer-experience"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5m",
    "description": "NASA-TLX adapted cognitive load assessments for platform tasks - measuring mental demand, effort, frustration, and overall workload",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "templating": {
      "list": [
        {
          "name": "task_type",
          "label": "Task Type",
          "type": "query",
          "query": "label_values(devex_nasa_tlx_overall_workload, task_type)",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "multi": true,
          "includeAll": true,
          "allValue": ".*",
          "current": { "text": "All", "value": "$__all" },
          "hide": 0,
          "refresh": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "NASA-TLX Overview",
        "type": "row",
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Overall Cognitive Workload",
        "type": "gauge",
        "gridPos": { "x": 0, "y": 1, "w": 8, "h": 6 },
        "description": "Average overall cognitive workload across all NASA-TLX dimensions (0-100 scale, lower is better). Target: <40 (healthy), <60 (warning), 60+ (critical)",
        "targets": [
          {
            "expr": "avg(devex_nasa_tlx_overall_workload{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 100,
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 40 },
                { "color": "orange", "value": 60 },
                { "color": "red", "value": 80 }
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 3,
        "title": "Total Assessments Submitted",
        "type": "stat",
        "gridPos": { "x": 8, "y": 1, "w": 8, "h": 3 },
        "description": "Total number of NASA-TLX assessments submitted",
        "targets": [
          {
            "expr": "sum(devex_nasa_tlx_submissions_total{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "id": 4,
        "title": "Average Performance Score",
        "type": "stat",
        "gridPos": { "x": 16, "y": 1, "w": 8, "h": 3 },
        "description": "Average performance score (0-100, higher is better). Indicates how successful users feel completing platform tasks.",
        "targets": [
          {
            "expr": "avg(devex_nasa_tlx_performance{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 100,
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "orange", "value": 50 },
                { "color": "yellow", "value": 70 },
                { "color": "green", "value": 85 }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Assessments by Task Type",
        "type": "piechart",
        "gridPos": { "x": 8, "y": 4, "w": 8, "h": 3 },
        "description": "Distribution of NASA-TLX assessments by task type",
        "targets": [
          {
            "expr": "sum by (task_type) (devex_nasa_tlx_submissions_total{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "pie",
          "displayLabels": ["percent"]
        }
      },
      {
        "id": 6,
        "title": "Workload by Task Type",
        "type": "bargauge",
        "gridPos": { "x": 16, "y": 4, "w": 8, "h": 6 },
        "description": "Average cognitive workload by task type - identify which platform tasks are most demanding",
        "targets": [
          {
            "expr": "avg by (task_type) (devex_nasa_tlx_overall_workload{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 100,
            "color": { "mode": "thresholds" },
