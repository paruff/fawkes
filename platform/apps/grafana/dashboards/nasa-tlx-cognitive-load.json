      {
        "id": 14,
        "title": "Trends Over Time",
        "type": "row",
        "gridPos": { "x": 0, "y": 12, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "id": 15,
        "title": "Overall Workload Trend",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 13, "w": 12, "h": 8 },
        "description": "Overall cognitive workload trend over time by task type",
        "targets": [
          {
            "expr": "avg by (task_type) (devex_nasa_tlx_overall_workload{task_type=~\"$task_type\"})",
            "refId": "A",
            "datasource": { "type": "prometheus", "uid": "prometheus" },
            "legendFormat": "{{task_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto",
              "spanNulls": false
            },
            "color": { "mode": "palette-classic" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 40 },
                { "color": "orange", "value": 60 },
                { "color": "red", "value": 80 }
              ]
            }
          }
        },
        "options": {
          "tooltip": { "mode": "multi" },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": ["mean", "last"]
          }
        }
      },
      {
        "id": 16,
        "title": "Frustration Trend",
