              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 40 },
                { "color": "orange", "value": 60 },
                { "color": "red", "value": 80 }
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 7,
        "title": "NASA-TLX Dimensions",
        "type": "row",
        "gridPos": { "x": 0, "y": 7, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
